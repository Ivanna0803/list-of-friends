          this.detailsAreVisible = !this.detailsAreVisible;\n        },\n        toggleFavorite() {\n            if(this.isFavorite === '1'){\n                this.isFavorite = '0'\n            } else {\n                this.isFavorite = '1';\n            }\n        }\n    }\n};\n</script>"],"mappings":"AAmBA,eAAe;EACXA,KAAK,EAAE,CACH,MAAM,EACN,aAAa,EACb,cAAc,EACd,YAAW,CACd;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,iBAAiB,EAAE,KAAK;MACxBC,MAAM,EAAE;QACAC,EAAE,EAAE,QAAQ;QACZC,IAAI,EAAE,eAAe;QACrBC,KAAK,EAAE,cAAc;QACrBC,KAAK,EAAE;MACX,CAAC;MACLC;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,aAAaA,CAAA,EAAG;MACZ,IAAI,CAACR,iBAAgB,GAAI,CAAC,IAAI,CAACA,iBAAiB;IACpD,CAAC;IACDS,cAAcA,CAAA,EAAG;MACb,IAAG,IAAI,CAACC,UAAS,KAAM,GAAG,EAAC;QACvB,IAAI,CAACA,UAAS,GAAI,GAAE;MACxB,OAAO;QACH,IAAI,CAACA,UAAS,GAAI,GAAG;MACzB;IACJ;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}